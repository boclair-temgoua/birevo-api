{
  "name": "berivo-api-v2",
  "version": "2.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "ngrok": "ngrok http http://localhost:3000",
    "husky:add-hook-commit-msg": "npx husky add .husky/commit-msg 'npx --no-install commitlint --edit \"$1\"'",
    "husky:add-hook-pre-commit": "npx husky add .husky/pre-commit 'npm run pre-commit'",
    "husky:add-hook-pre-push": "npx husky add .husky/pre-push 'npm run pre-push'",
    "pre-commit": "lint-staged && git remote prune origin",
    "postinstall": "npm run prepare",
    "lint": "eslint --max-warnings 0 --ext .js,ts src",
    "lint-prettier": "prettier --config .prettierrc --list-different '{src,test}/**/*.{ts,js,css,scss}'",
    "prettier": "prettier --config .prettierrc --write '{src,test}/**/*.{ts,js,css,scss}'",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "docker:dev:start": "docker-compose --file docker-compose.yml --compatibility up --no-recreate",
    "docker:dev:rebuild": "docker-compose --file docker-compose.yml --compatibility up --build",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/infrastructure/databases/config/orm-config.ts",
    "typeorm:genrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/infrastructure/databases/config/orm-config.ts",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:generate": "npm run typeorm:genrate migration:generate ./src/infrastructure/databases/migrations/$npm_config_name -n",
    "migration:create": "cross-var ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/infrastructure/databases/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm migration:revert",
    "seed:run": "NODE_PATH=./src ts-node ./node_modules/.bin/typeorm migration:run --config ./src/providers/typeorm/config/ormconfig-seed.ts",
    "seed:create": "NODE_PATH=./src ts-node ./node_modules/.bin/typeorm migration:create --config ./src/providers/typeorm/config/ormconfig-seed.ts -n "
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.9",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.9",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.0.9",
    "@nestjs/platform-express": "^9.0.9",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/terminus": "^9.1.1",
    "@nestjs/throttler": "^3.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "amqp-connection-manager": "^4.1.6",
    "amqplib": "^0.10.2",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-var": "^1.1.0",
    "csurf": "^1.11.0",
    "dayjs": "^1.11.5",
    "handlebars": "^4.7.7",
    "helmet": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "nestjs-real-ip": "^2.2.0",
    "nodemailer": "^6.7.8",
    "number-generator": "^4.0.6",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "stripe": "^10.2.0",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2",
    "xlsx": "^0.18.5",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/testing": "^9.0.9",
    "@types/amqplib": "^0.8.2",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.2",
    "@types/nodemailer": "^6.4.5",
    "@types/passport-jwt": "^3.0.6",
    "@types/qrcode": "^1.4.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "dotenv": "16.0.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "passport-jwt": "^4.0.0",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
