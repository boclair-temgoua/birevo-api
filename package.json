{
  "name": "berivo-api-v2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "ngrok": "ngrok http http://localhost:4600",
    "husky:add-hook-commit-msg": "npx husky add .husky/commit-msg 'npx --no-install commitlint --edit \"$1\"'",
    "husky:add-hook-pre-commit": "npx husky add .husky/pre-commit 'npm run pre-commit'",
    "husky:add-hook-pre-push": "npx husky add .husky/pre-push 'npm run pre-push'",
    "pre-commit": "lint-staged && git remote prune origin",
    "postinstall": "npm run prepare",
    "lint": "eslint --max-warnings 0 --ext .js,ts src",
    "lint-prettier": "prettier --config .prettierrc --list-different '{src,test}/**/*.{ts,js,css,scss}'",
    "prettier": "prettier --config .prettierrc --write '{src,test}/**/*.{ts,js,css,scss}'",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "docker:dev:start": "docker-compose --file docker-compose.yml --compatibility up --no-recreate",
    "docker:dev:rebuild": "docker-compose --file docker-compose.yml --compatibility up --build",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/infrastructure/databases/config/orm-config.ts",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:generate": "npm run typeorm migration:generate ./src/infrastructure/databases/migrations/$npm_config_name -n",
    "migration:create": "npm run typeorm migration:create ./src/infrastructure/databases/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.7",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/microservices": "^9.0.7",
    "@nestjs/platform-express": "^9.0.7",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/terminus": "^9.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "amqp-connection-manager": "^4.1.3",
    "amqplib": "^0.10.1",
    "cross-var": "^1.1.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.7",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.6",
    "@types/node": "^18.6.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "dotenv": "16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,scss,json,md}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
